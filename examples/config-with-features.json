{
    "proxy": {
        "stealth": true,
        "cloudflare": false,
        "maxLogLength": 5,
        "adminsecret": "CHANGE_ME",
        "apisecret": "CHANGE_ME",
        "secrets": {
            "captcha": "CHANGE_ME1",
            "cookie": "CHANGE_ME2",
            "javascript": "CHANGE_ME3"
        },
        "timeout": {
            "idle": 5,
            "read": 5,
            "write": 7,
            "read_header": 5
        },
        "ratelimits": {
            "challengeFailures": 40,
            "noRequestsSent": 10,
            "requests": 500,
            "unknownFingerprint": 150
        },
        "colors": [
            "0",
            "31"
        ],
        "metrics": {
            "enabled": true,
            "port": 9090,
            "path": "/metrics"
        },
        "deduplication": {
            "enabled": true,
            "ttl_seconds": 30
        }
    },
    "domains": [
        {
            "name": "example.com",
            "backend": "127.0.0.1:8080",
            "scheme": "http",
            "certificate": "assets/server/server.crt",
            "key": "assets/server/server.key",
            "webhook": {
                "url": "https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN",
                "name": "balooProxyX",
                "avatar": "https://avatars.githubusercontent.com/u/73783549",
                "attack_start_msg": "ðŸš¨ DDoS attack detected on `{{domain.name}}` at {{attack.start}}. CPU: {{proxy.cpu}}%, RAM: {{proxy.ram}}%",
                "attack_stop_msg": "âœ… DDoS attack on `{{domain.name}}` ended at {{attack.end}}. Duration: {{attack.duration}}. Peak: {{attack.peak_rps}} req/s"
            },
            "firewallRules": [
                {
                    "expression": "(http.path eq \"/captcha\")",
                    "action": "3"
                },
                {
                    "expression": "(http.path eq \"/js\")",
                    "action": "2"
                },
                {
                    "expression": "(ip.engine eq \"\" or ((ip.engine eq \"Firefox\" and http.user_agent contains \"chrome\") or (ip.engine eq \"Chromium\" and http.user_agent contains \"firefox\")))",
                    "action": "+1"
                },
                {
                    "expression": "(http.method ne \"GET\" and http.method ne \"POST\")",
                    "action": "+2"
                }
            ],
            "bypassStage1": 75,
            "bypassStage2": 250,
            "disableBypassStage3": 100,
            "disableRawStage3": 250,
            "disableBypassStage2": 50,
            "disableRawStage2": 75,
            "stage2Difficulty": 5
        }
    ]
}
